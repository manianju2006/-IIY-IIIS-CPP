#include <iostream> 
#include <vector> 
#include <list> 
using namespace std; 
int main() { 
    vector<int> vec; 
    vec.push_back(10); 
    vec.push_back(20); 
    vec.push_back(30); 
    vec.insert(vec.begin() + 1, 15); 
    vec.pop_back(); 
    cout << "Vector access at index 1: " << vec[1] << "\n"; 
    cout << "Vector elements: "; 
    for (vector<int>::iterator vit = vec.begin(); vit != vec.end(); ++vit) 
{ 
        cout << *vit << " "; 
    } 
    list<int> lst; 
    lst.push_back(100); 
    lst.push_front(50); 
    list<int>::iterator it = lst.begin(); 
    advance(it, 1); 
    lst.insert(it, 75); 
    lst.pop_back(); 
    it = lst.begin(); 
    advance(it, 1); 
    cout << "\nList access at second position: " << *it << "\n"; 
    cout << "List elements: "; 
    for (list<int>::iterator lit = lst.begin(); lit != lst.end(); ++lit) { 
        cout << *lit << " "; 
    } 
    return 0; 
}
