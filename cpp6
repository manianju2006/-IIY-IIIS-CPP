#include <iostream>
using namespace std;
class Student {
private:
    string name;
    int rollNo;
public:
    void setData(string n, int r) {
        name = n;
        rollNo = r;
    }
    void displayData() {
        cout << "Name: " << name << endl;
        cout << "Roll Number: " << rollNo << endl;
    }
};
int main() {
    Student s1;
    s1.setData("Satya Sree", 180);
    s1.displayData();
    return 0;
}

                        
